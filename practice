var searchterms = polyjs.data({
    term: ["hope", "honesty", "joy", "luck", "other"],
    count: [16, 10, 12, 5, 7 ]
});
polyjs.chart({
    layer: {
        data: searchterms,
        type: "bar",
        x: "term",
        y: "count"
    },
    title: "Monthly Google Search",
    dom: "chart",
    width: 300,
    height: 250
});
